#include <stdio.h> 
 int main() { 
    int num1 = 2; 
    int num2 = 3; 
    int sum; 
    int num3 = 5; 
    int num4 = 3; 
    int difference; 
    difference = num3 - num4; 
    printf("The difference between %d and %d is %d\n", num3, num4, difference); 
 
    sum = num1 + num2; 
    printf("The sum of %d and %d is %d\n", num1, num2, sum); 
 
    return 0; 
} 
